   "source": [
    "fig, ax = plt.subplots(1, 1)\n",
    "mu = 5 \n",
    "x = np.arange(0, 15, 1)\n",
    "p=poisson.pmf(x, mu)\n",
    "ax.plot(x, p, 'bo', ms=4, label='poisson pmf')\n",
    "ax.vlines(x, 0, poisson.pmf(x, mu), colors='b', lw=1, alpha=0.5)\n",
    "plt.legend()"
   ]
  },
