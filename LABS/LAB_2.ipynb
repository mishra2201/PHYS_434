   "execution_count": 3,
   "id": "4bb7d115-6c34-4743-9c14-7957c7b4d918",
   "metadata": {},
   "outputs": [],
   "source": [
    "n=2\n",
    "conv_1=np.convolve(p,p)\n",
    "x1=np.linspace(0,n*15,len(conv_1))\n",
